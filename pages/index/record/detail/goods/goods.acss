.goods{
  background: #fff;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  overflow-y: scroll;
  padding-left: 30rpx;
  padding-right: 30rpx;
  -webkit-overflow-scrolling:touch;
}
.goodsItem{
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 90rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #f4f4f4;
}
.num{
  font-size: 28rpx;
  color: #999;
}